<div class="wrap-login row">

    <div class="login login-left">
        <% for( let i = 0; i < 10; i++ ) { %>
        <h5>All work and no play makes Jack a dull boy.</h5>
        <% } %>
        
    </div>
    
    <div class="login login-right">
        <div class="header">
            <img src="/images/logo-dark.png" alt="" id="login-logo-left"><h2>HASTE</h2>
        </div>    
        <p style="text-align: center;">Welcome back! Log in to manage today's deliveries.</p>

        <form action="/auth/login" method="POST">
            <input name="email" type="text" class="login-input" placeholder="Email">
            <input name="password" type="password" class="login-input" placeholder="Password">
            <button class="login-btn">LOG IN</button>
        </form>

    </div>

</div>